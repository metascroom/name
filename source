local player = game.Players.LocalPlayer
local chat = player:FindFirstChild("PlayerGui"):FindFirstChild("ChatBar", true)
local duration = .05
local slurs = {
"I Rape My Children",
"I Rape My Children",
"I Rape My Children",
"I Rape My Children",
"I Rape My Children",
"I Rape My Children",
"I Rape My Children",
"I Rape My Children",
"I Rape My Children",
"I Rape My Children",
"I Rape My Children",
"I Rape My Children",
"I Rape My Children",
}

while task.wait(duration) do
chat:CaptureFocus()
chat.Text = slurs[math.random(1, #slurs)]
chat:ReleaseFocus(true)
end
